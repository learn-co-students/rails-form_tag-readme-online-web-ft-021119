<h3>Post Form</h3>
<%= form_tag posts_path do %>
    <label>Post title:
        <%= text_field_tag :'post[title]' %>
    </label>
    <br>
    <label>Post description:
        <%= text_area_tag :'post[description]' %>
    </label>
    <br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Submit Post" %>
<% end %>

<%= params.inspect %>